{
  "name": "es6-karma-jasmine-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ES6 client-based library. Uses Babel and webpack for transpiling and building, Karma and Jasmine for unit testing, and istanbul-instrumenter-loader for code coverage.",
  "main": "index.js",
  "scripts": {
    "groc": "rimraf docs/groc && groc",
    "charset-euckr": ". ./tools/encoding-euckr.sh",
    "test": "karma start",
    "test-start": "NODE_ENV=dev karma start",
    "start": "webpack-dashboard -c magenta -- webpack-dev-server --config ./tools/webpack.config.js --env=dev --progress --profile --colors",
    "prebuild": "rimraf dist/ && npm run test",
    "build": "webpack --config ./tools/webpack.config.js --env=prd --progress --profile --colors",
    "postbuild": "npm run groc"
  },
  "repository": {
    "type": "git",
    "url": "http://1003419@bitbucket.skplanet.com/scm/stui/es6-karma-jasmine-webpack-boilerplate.git"
  },
  "keywords": [
    "es6",
    "babel",
    "webpack",
    "jasmine",
    "karma",
    "istanbul",
    "boilerplate"
  ],
  "author": "poohding (kjy2143@sk.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "core-js": "^2.5.1",
    "es3ify-loader": "^0.2.0",
    "es5-shim": "^4.5.9",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "eslint-watch": "^3.1.2",
    "groc": "^0.8.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-mocha-reporter": "^2.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "loader-utils": "^1.1.0",
    "rimraf": "^2.6.1",
    "sponge": "^0.1.0",
    "webpack": "^3.5.5",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {}
}
